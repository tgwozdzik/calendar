    "rimraf": "^2.5.4",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "lodash": "4.11.1",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "babel-runtime": "6.x",
    "classnames": "2.x",
