{
  "name": "@tgwozdzik/react-calendar",
  "version": "0.1.0",
  "description": "Calendar component for React",
  "keywords": [
    "react",
    "calendar",
    "component",
    "ui",
    "tg"
  ],
  "files": [
    "lib",
    "es",
    "assets/*.css",
    "dist"
  ],
  "homepage": "",
  "author": "Tomasz Gwo≈∫dzik <tomasz.gwozdzik@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tgwozdzik/react-calendar.git"
  },
  "bugs": "https://github.com/tgwozdzik/react-calendar/issues",
  "license": "MIT",
  "config": {
    "port": 8000,
    "entry": {
      "rc-calendar": [
        "./assets/index.less",
        "./src/index.js"
      ]
    }
  },
  "main": "lib/index.js",
  "scripts": {
    "clean:build": "node ./bin/clean.js createdir",
    "build:html": "node ./bin/buildHtml.js",
    "prestart": "node ./bin/clean.js",
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --hot --inline --progress",
    "prebuild": "npm run clean:build && npm run build:html",
    "build": "export NODE_ENV=production & NODE_ENV=production & webpack -p --config ./config/webpack.config.prod.js --progress",
    "prepublish": "babel src --out-dir ./lib"
  },
  "devDependencies": {
    "async": "~2.5.0",
    "babel-cli": "^6.10.1",
    "babel-core": "6.7.7",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "bootstrap": "~3.3.2",
    "cheerio": "0.20.0",
    "colors": "1.1.2",
